<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.recycleBin {
			background-image: url('images/trashcan.jpeg');
			width: 100px;
			height: 100px;
			position: absolute;
			bottom: 0;
		}
	</ui:style>
	<g:FlowPanel ui:field="recycleBin" ui:styleName="{style.recycleBin}">
	</g:FlowPanel>
</ui:UiBinder> 